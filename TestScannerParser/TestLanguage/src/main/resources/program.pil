import "esmalib.pil";
prototype void megafedt(int yes.val);
prototype void megalort(int no.val);

int a = 67;
int b = 5;
b.val = 3;
a.val = b.val;

if(a.val > b.val) {
    megafedt(a.val);
}else{
    megalort(b.val);
}

void megafedt(int yes.val){
    if(yes.val IS NOT 0){
        print("jaja, det lort virker sku!");
    }else{
        megalort(yes.val);
    }
}

void megalort(int no.val){
    if(no.val IS 0){
        print("nej det er sku lort");
    }else{
        megafedt(no.val);
    }
}